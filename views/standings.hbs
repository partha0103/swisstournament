<table class="table table-striped table-inverse st_table table-hover">
    <thead class="thead-inverse bg-info">
        <tr>
            <th>Name</th>
            <th>Matches</th>
            <th>Wins</th>
            <th>Losses</th>
            <th>Round</th>
        </tr>
    </thead>
    <tbody class="st_body">
        {{#each standings}}
            <tr>
                <td>{{name}}</td>
                <td>{{no_matches}}</td>
                <td>{{wins}}</td>
                <td>{{losses}}</td>
                <td>{{no_matches}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>
